# Hyperliquid Configuration
VITE_HYPERLIQUID_NETWORK=mainnet  # or testnet for testing

# 0x Protocol API (for swaps)
VITE_ZERO_X_API_KEY=your_0x_api_key_here

# Supabase (auto-configured by Lovable)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# MoonPay (for fiat on/off ramp)
VITE_MOONPAY_API_KEY=your_moonpay_api_key

# Optional: RPC Endpoints (defaults provided in code)
# VITE_ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY
# VITE_ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/YOUR_KEY
# VITE_POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_KEY

# Feature Flags
VITE_ENABLE_TRADING=true
VITE_ENABLE_LENDING=true
VITE_ENABLE_HYPERLIQUID=true

# Security
VITE_ENABLE_RATE_LIMITING=true
VITE_MAX_REQUESTS_PER_MINUTE=60
